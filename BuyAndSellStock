# Java-Programming

import java.util.Arrays;

public class BuyAndSellStock {
    public static void main(String[] args) {
        int a[]={2,1,4};
        int min=Integer.MAX_VALUE,max=-1;
        for(int i=0;i<a.length;i++)
        {
            if(a[i]<min)
            {
                min=a[i];
            }
            else if(a[i]-min>max)
            {
                max=a[i]-min;
            }
        }
        System.out.println(max<=0?-1:max);



    }
}
